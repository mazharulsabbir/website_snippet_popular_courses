<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Frontend Assets -->
    <template id="web_assets_frontend" name="web_assets_frontend" inherit_id="web.assets_frontend">
        <xpath expr="." position="inside">
            <link href="/website_snippet_popular_courses/static/src/css/main.css" rel="stylesheet" type="text/css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/website_snippet_popular_courses/static/src/js/course.js"/>
        </xpath>
    </template>

</odoo>